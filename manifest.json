{
    "manifest_version": 3,
    "name": "tabs Lock",
    "version": "1.0",
    "description": "Locks the browser with a password",
    "permissions": ["storage", "activeTab", "tabs", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ]
  }
